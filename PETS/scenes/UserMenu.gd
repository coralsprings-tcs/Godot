extends Control

onready var MustardHam = $feed/MustardHam
onready var MustardTimer = $feed/MustardVisible

func _ready():
	# set invisible
	MustardHam.visible = false

func _on_FeedButton_pressed():
	# make visible
	MustardHam.visible = true
	
	if MustardHam.visible:
		MustardTimer.start()
		yield(MustardTimer,"timeout")
		
		# TODO: look up how to get parent node
		print(get_parent_node().hunger)
		#\\\\show it worked
		print('feed should be invisible')
		
		# feed is now invisible
		MustardHam.visible = false
